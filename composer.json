{
	"name" : "mouf/utils.common.doctrine-annotations-wrapper",
	"description" : "Provide an install script for a Doctrine's AnnotationReader in Mouf Framework.",
	"keywords" : [
		"doctrine",
		"annotations",
		"wrapper",
		"mouf"
	],
	"homepage" : "https://mouf-php.com/packages/mouf/utils.common.doctrine-annotations-wrapper",
	"type" : "mouf-library",
	"license" : "MIT",
	"authors" : [{
			"name" : "Camille Koppel",
			"email" : "c.koppel@thecodingmachine.com",
			"homepage" : "http://mouf-php.com"
		}
	],
	"require" : {
		"php" : ">=5.3.0",
		"doctrine/annotations" : "~1.2",
		"doctrine/cache": "~1.4"
	},
	"autoload" : {
		"psr-4" : {
			"Mouf\\Utils\\Common\\Doctrine\\Annotations\\" : "src/"
		}
	},
	"extra" : {
		"mouf" : {
			"install" : [{
					"type" : "class",
					"class": "Mouf\\Utils\\Common\\Doctrine\\Annotations\\AnnotationReaderInstaller"
				}
			],
			"logo" : "logo.png"
		}
	}
}